# Copy this file to .env and fill in your values
# .env is gitignored and will never be committed
# .env.example is safe to commit — it contains no real secrets

# Gmail address you're sending FROM (the one you created for this agent)
EMAIL_SENDER=your_agent_gmail@gmail.com

# Gmail App Password — NOT your regular password
# Get one at: myaccount.google.com → Security → App Passwords
EMAIL_PASSWORD=xxxx xxxx xxxx xxxx

# Where you want to receive the daily digest
EMAIL_RECIPIENT=your_personal_email@gmail.com

# Ollama settings — defaults are fine if running locally
OLLAMA_URL=http://localhost:11434/api/generate
OLLAMA_MODEL=qwen2.5:14b

# Salary filter (in dollars, no commas)
SALARY_MIN=80000
SALARY_MAX=130000
